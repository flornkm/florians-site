{
    "cleanUrls": true,
    "rewrites": [
        { "source": "/globe", "destination": "/globe/index.html" },
        { "source": "/resize/:width/:height", "destination": "/api/sharp" },
        { "source": "/proxy/:match*", "destination": "https://example.com/:match*" },
        {
          "source": "/:path((?!uk/).*)",
          "has": [
            {
              "type": "header",
              "key": "x-vercel-ip-country",
              "value": "GB"
            }
          ],
          "destination": "/uk/:path*"
        }
      ]
    }