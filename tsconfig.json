{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "module": "ESNext",
    "noEmit": true,
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client", "vike-react", "vite-plugin-vercel/types"],
    "jsx": "preserve",
    "jsxImportSource": "react",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "exclude": ["dist"]
}
